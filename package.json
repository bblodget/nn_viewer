{
  "name": "nncircuit",
  "version": "2.0.0",
  "description": "Interactive neural network schematic diagram viewer",
  "main": "dist/nncircuit.js",
  "module": "dist/nncircuit.esm.js",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint src",
    "serve": "http-server -c-1"
  },
  "keywords": [
    "neural-network",
    "visualization",
    "d3js",
    "schematic"
  ],
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@testing-library/jest-dom": "^6.1.4",
    "babel-jest": "^29.7.0",
    "cypress": "^13.3.1",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^8.53.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^3.29.3",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "ajv": "^8.12.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/vendor/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ]
  }
}